<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<!-- Phần cấu hình runtime của ứng dụng -->
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>

	<!-- Phần cấu hình ràng buộc thư viện -->
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="System.Text.Json" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-8.0.0.5" newVersion="8.0.0.5" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>

	<!-- Phần Entity Framework (nếu có sử dụng sau này) -->
	<entityFramework>
		<providers>
			<provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
		</providers>
	</entityFramework>

	<!-- Tạm thời KHÔNG dùng kết nối SQL, để trống connectionStrings -->
	<connectionStrings>
		<!-- Khi nào kết nối SQL thì bật dòng dưới đây -->
		<!--
    <add name="conn"
         connectionString="Data Source=localhost\\SQLEXPRESS;Initial Catalog=QuanLyQuanBar;Integrated Security=True;"
         providerName="System.Data.SqlClient" />
    -->
	<add name="QuanlyquanbarDB" connectionString="data source=DESKTOP-BVQNC89;initial catalog=Quanlyquanbar;integrated security=True;trustservercertificate=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" /><add name="QuanLyQuanBarContext" connectionString="data source=DESKTOP-BVQNC89;initial catalog=Quanlyquanbar;integrated security=True;trustservercertificate=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" /></connectionStrings>
</configuration>
